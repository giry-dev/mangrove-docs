    "@docusaurus/preset-classic": "^2.0.0",
    "@mangrovedao/mangrove.js": "^0.11.4",
    "@mdx-js/react": "^1.6.22",
    "@vegaprotocol/docusaurus-theme-github-codeblock": "^0.1.8",
    "clsx": "^1.2.1",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "hast-util-is-element": "1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-katex": "5",
    "remark-math": "3",
    "solcco": "^1.0.12",
    "ts-essentials": "^9.3.0",
    "typechain": "^8.1.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "vercel": "^28.4.12"
