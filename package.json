{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn install && docusaurus start",
    "build": "yarn install && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "predocgen": "bash git diff-index --quiet HEAD -- || echo \"You have uncommitted changes\"",
    "docgen": "(git diff-index --quiet HEAD -- || (echo \"You have uncommitted changes\" && exit 1)) && yarn natspec && yarn doccore && docusaurus parse && docusaurus glossary && docusaurus build",
    "natspec": "yarn copy-solidity && yarn hardhat docgen && yarn remove-solidity",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "addresses-to-md": "node scripts/addresses-to-md.js",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "copy-solidity": "cp -R node_modules/@mangrovedao/mangrove-core/src ./contracts",
    "postinstall": "cp patches/scope.js node_modules/solidity-docgen/dist/utils",
    "doccore": "rm -rf static/MgvDoc && cp -r node_modules/@mangrovedao/mangrove-core/doc/ static/MgvDoc && rm static/MgvDoc/index.html && mv static/MgvDoc/MgvDoc.html static/MgvDoc/index.html",
    "remove-solidity": "rm -R ./contracts"
  },
  "dependencies": {
    "@akebifiky/remark-simple-plantuml": "^1.0.2",
    "@docusaurus-terminology/parser": "^1.3.0",
    "@docusaurus-terminology/term": "^1.0.0",
    "@docusaurus/core": "^2.1.0",
    "@docusaurus/plugin-content-docs": "^2.0.0",
    "@docusaurus/plugin-ideal-image": "^2.2.0",
    "@docusaurus/preset-classic": "^2.0.0",
    "@docusaurus/theme-mermaid": "^2.2.0",
    "@mangrovedao/mangrove-core": "^1.1.3",
    "@mangrovedao/mangrove.js": "^0.12.0",
    "@mdx-js/react": "^1.6.22",
    "@vegaprotocol/docusaurus-theme-github-codeblock": "^0.1.8",
    "clsx": "^1.2.1",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "hast-util-is-element": "1.1.0",
    "postinstall": "^0.8.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-katex": "5",
    "remark-math": "3",
    "solidity-docgen": "^0.6.0-beta.29",
    "ts-essentials": "^9.3.0",
    "typechain": "^8.1.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4",
    "vercel": "^28.4.12"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0",
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "docusaurus-lunr-search": "^2.3.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.12.2",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-preprocessor": "^0.1.5",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "yarn@3.3.0",
  "resolutions": {
    "@docusaurus-terminology/parser@^1.3.0": "patch:@docusaurus-terminology/parser@npm%3A1.3.0#./.yarn/patches/@docusaurus-terminology-parser-npm-1.3.0-aec379a90d.patch",
    "solidity-docgen@^0.6.0-beta.29": "patch:solidity-docgen@npm%3A0.6.0-beta.30#./.yarn/patches/solidity-docgen-npm-0.6.0-beta.30-bf73aface8.patch",
    "@vegaprotocol/docusaurus-theme-github-codeblock@^0.1.8": "patch:@vegaprotocol/docusaurus-theme-github-codeblock@npm%3A0.1.8#./.yarn/patches/@vegaprotocol-docusaurus-theme-github-codeblock-npm-0.1.8-70d8c7434d.patch"
  }
}
